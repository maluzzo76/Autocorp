					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureMySqlSource",
						"query": "SELECT distinct id FROM freshdesk_tickets \r\nWHERE  created_at > DATE_SUB(CURDATE(), INTERVAL 36 DAY) \r\nlimit 10;",
						"queryTimeout": "02:00:00"
					},
					"dataset": {
						"referenceName": "FreshTickets",
						"type": "DatasetReference"
					},
