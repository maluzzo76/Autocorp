	"name": "006-Inyectar_Echos",
	"properties": {
		"activities": [
			{
				"name": "Inyectar_Dw_FactTickets",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
